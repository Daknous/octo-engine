{
  "name": "suitest-js-api-mocha",
  "version": "1.0.0",
  "description": "Suitest JS API",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^5.2.0",
    "form-data": "^4.0.0",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "suitest-js-api": "^3.1.2"
  },
  "scripts": {
    "smoke:test": "suitest run mocha --preset 1und1 --preset zattooch --log-dir=e2e mocha --no-timeouts --exit \"./test/bdd.test.js\"",
    "zapreport": "suitest node --reporter mochawesome --no-timeouts --exit \"./test/dummy.test.js\"",
    "interactive": "suitest interactive mocha --reporter mocha-junit-reporter --no-timeouts --exit \"./test/dummy.test.js\"",
    "interactive:logs": "suitest interactive --log-dir=e2e mocha --no-timeouts --exit \"./test/some.test.js\"",
    "interactive:debug": "suitest interactive --inspect-brk=9121 node ./node_modules/mocha/bin/_mocha --no-timeouts --exit \"./test/debug.test.js\"",
    "interactive:repl": "suitest interactive mocha --no-timeouts --exit \"./test/dummy.test.js\"",
    "automated": "suitest automated mocha --no-timeouts --exit \"./test/dummy.test.js\"",
    "automated:logs": "suitest automated --log-dir=e2e mocha --no-timeouts --exit \"./test/dummy.test.js\"",
    "dev": "suitest interactive --no-timeouts --exit \"./test/bdd.test.js\"",
    "dev:test": "suitest interactive mocha --reporter mochawesome --log-dir=e2e mocha --no-timeouts --exit \"./test/smoke.test.js\"",
    "development:test": "suitest interactive mocha --log-dir=e2e mocha --no-timeouts --exit ./test/bdd.test.js",
    "zapi:axios": "node \"./scripts/axios.js\""
  },
  "devDependencies": {
    "mocha-ci-slack-reporter": "^1.0.1",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.0",
    "xunit-viewer": "^7.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daknous/octo-engine.git"
  },
  "keywords": [],
  "author": "Seif Daknou <seif.daknou@zattoo.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daknous/octo-engine/issues"
  },
  "homepage": "https://github.com/Daknous/octo-engine#readme"
}
